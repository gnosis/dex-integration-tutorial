{
  "name": "dex-integration",
  "version": "1.0.0",
  "description": "An tiny integration example demonstrating how to place spread orders on Gnosis Protocol from an external price feed",
  "author": "ben@gnosis.io",
  "license": "MIT",
  "keywords": [
    "Gnosis Protocol",
    "Integration",
    "Tutorial"
  ],
  "scripts": {
    "build": "yarn compile && yarn copy-artifacts",
    "compile": "truffle compile",
    "copy-artifacts": "cp ./node_modules/@gnosis.pm/dex-contracts/build/contracts/BatchExchange.json ./build/contracts"
  },
  "dependencies": {
    "@gnosis.pm/dex-contracts": "^0.5.0",
    "@gnosis.pm/util-contracts": "^2.0.6",
    "@openzeppelin/contracts": "=4.4.2",
    "@truffle/artifactor": "^4.0.70",
    "synthetix-js": "=2.30.1"
  },
  "devDependencies": {
    "yargs": "^15.3.1"
  }
}
